            }
          }
        },
        "modules": {
          "ImageCapture": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "rpipoccontainerregistry.azurecr.io/imagecapture:0.0.2-arm32v7",
              "createOptions": "{}"
            }
          },
          "SimulatedTemperatureSensor": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
